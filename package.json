{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "razzle start",
    "start:prod": "NODE_ENV=production node build/server.js",
    "build": "razzle build",
    "lint": "eslint --format pretty --ext .ts --ext .tsx src",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "test": "razzle test --env=jsdom",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@logto/eslint-config": "^0.1.0-rc.6",
    "@logto/eslint-config-react": "^0.1.0-rc.7",
    "@logto/ts-config": "^0.1.0-rc.6",
    "@logto/ts-config-react": "^0.1.0-rc.7",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/webpack-env": "^1.16.2",
    "babel-preset-razzle": "4.0.5",
    "eslint": "^7.30.0",
    "eslint-formatter-pretty": "^4.1.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.1",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.3.2",
    "razzle": "^4.0.5",
    "razzle-dev-utils": "^4.0.5",
    "razzle-plugin-scss": "^4.0.5",
    "stylelint": "^13.13.1",
    "typescript": "^4.3.5",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.2"
  },
  "eslintConfig": {
    "extends": "@logto/react"
  },
  "stylelint": {
    "extends": "@logto/eslint-config-react/.stylelintrc"
  },
  "prettier": "@logto/eslint-config/.prettierrc",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --format pretty --cache --fix",
    "*.scss": "stylelint --fix"
  }
}
